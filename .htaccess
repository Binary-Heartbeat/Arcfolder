# Begin Arcfolder Entries #

# These entries are for security purposes. It is not recommended to change them.
<filesMatch ^(config.php|settings.php)$>
Order Deny,Allow
Deny from all
Allow from 127.0.0.1
</filesMatch>

# Rewrite engine ftw?
RewriteEngine On
# RewriteBase /public/arcfolder/ # Not necessary. Yet. Keeping it for later since mod_rewrite is a bitch
RewriteRule ^([^/\.]+)$ index.php?p=$1 [L]
